<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.keepdays.weidaka" version="0.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>微打卡</name>
  <description>
        微打卡 － 一个专注于目标管理和习惯养成的打卡工具
    </description>
  <author email="admin@keepdays.com" href="http://www.keepdays.com/">
      Keepdays Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="*"/>
  <allow-navigation href="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="1000"/>
  <preference name="QQAppId" value="1104758278"/>
  <preference name="WeiboAppId" value="1114202144"/>
  <preference name="wechatappid" value="wx1bdc17e4439c5ffc"/>
  <preference name="REDIRECTURI" value="http://www.keepdays.com"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <platform name="ios">
    <icon height="57" src="resources/ios/icon/icon.png" width="57"/>
